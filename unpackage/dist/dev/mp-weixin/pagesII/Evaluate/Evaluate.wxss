	margin-left: 20rpx;
	color: #707070;
}
.tui-tab-rank-cent{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	padding: 20rpx ;
}
.img-rink{
	width: 180rpx;
	height: 180rpx;
	display: block;
	margin-right: 20rpx;
}
.tui-pro-tit {
	-webkit-box-flex: 5;
	-webkit-flex: 5;
	        flex: 5;
}
.tag-tit{
	/* 渐变色 */
	background-image: -webkit-gradient(linear, left top, right top, from(#00C94A) , to(#00AC3F));
	background-image: -webkit-linear-gradient(left, #00C94A , #00AC3F);
	background-image: linear-gradient(to right, #00C94A , #00AC3F);
	margin-right: 10rpx;
	padding: 0 10rpx;
	-webkit-border-radius: 15rpx 0 15rpx 0;
	        border-radius: 15rpx 0 15rpx 0;
	color: #fff;
	font-size: 20rpx;
}
.tag-tit2{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.tag-tit2-price{
	color: #555555;
	font-size: 24rpx;
}
.tag-tit2-text{
